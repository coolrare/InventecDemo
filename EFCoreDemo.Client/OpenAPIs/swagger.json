  "components": {
    "schemas": {
      "CourseCreateRequestDto": {
        "required": [
          "title"
        ],
        "type": "object",
        "properties": {
          "title": {
            "minLength": 3,
            "type": "string"
          },
          "credits": {
            "maximum": 5,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "departmentId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "CourseCreateResponseDto": {
        "type": "object",
